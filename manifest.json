{
    "name": "scrolldown",
    "description": "scroll down the page",
    "version": "0.1",
    "permissions": [
        "activeTab",
        "<all_urls>",
        "webNavigation"
    ],
    "browser_action": {
        "default_icon": "images/get_started16.png"
    },
   
    "background": {
       "scripts": [
            "integrated_back.js"
        ],
       "persistent": true
    },
    "manifest_version": 2
}